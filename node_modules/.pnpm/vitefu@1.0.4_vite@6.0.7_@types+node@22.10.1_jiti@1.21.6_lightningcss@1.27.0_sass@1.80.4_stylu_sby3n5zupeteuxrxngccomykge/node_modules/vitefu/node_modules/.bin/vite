#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/re6ce/Templates/fuwari/node_modules/.pnpm/vite@6.0.7_@types+node@22.10.1_jiti@1.21.6_lightningcss@1.27.0_sass@1.80.4_stylus@0.63.0_terser@5.36.0_yaml@2.6.1/node_modules/vite/bin/node_modules:/home/re6ce/Templates/fuwari/node_modules/.pnpm/vite@6.0.7_@types+node@22.10.1_jiti@1.21.6_lightningcss@1.27.0_sass@1.80.4_stylus@0.63.0_terser@5.36.0_yaml@2.6.1/node_modules/vite/node_modules:/home/re6ce/Templates/fuwari/node_modules/.pnpm/vite@6.0.7_@types+node@22.10.1_jiti@1.21.6_lightningcss@1.27.0_sass@1.80.4_stylus@0.63.0_terser@5.36.0_yaml@2.6.1/node_modules:/home/re6ce/Templates/fuwari/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/re6ce/Templates/fuwari/node_modules/.pnpm/vite@6.0.7_@types+node@22.10.1_jiti@1.21.6_lightningcss@1.27.0_sass@1.80.4_stylus@0.63.0_terser@5.36.0_yaml@2.6.1/node_modules/vite/bin/node_modules:/home/re6ce/Templates/fuwari/node_modules/.pnpm/vite@6.0.7_@types+node@22.10.1_jiti@1.21.6_lightningcss@1.27.0_sass@1.80.4_stylus@0.63.0_terser@5.36.0_yaml@2.6.1/node_modules/vite/node_modules:/home/re6ce/Templates/fuwari/node_modules/.pnpm/vite@6.0.7_@types+node@22.10.1_jiti@1.21.6_lightningcss@1.27.0_sass@1.80.4_stylus@0.63.0_terser@5.36.0_yaml@2.6.1/node_modules:/home/re6ce/Templates/fuwari/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vite@6.0.7_@types+node@22.10.1_jiti@1.21.6_lightningcss@1.27.0_sass@1.80.4_stylus@0.63.0_terser@5.36.0_yaml@2.6.1/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../vite@6.0.7_@types+node@22.10.1_jiti@1.21.6_lightningcss@1.27.0_sass@1.80.4_stylus@0.63.0_terser@5.36.0_yaml@2.6.1/node_modules/vite/bin/vite.js" "$@"
fi
